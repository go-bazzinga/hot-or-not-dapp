{
	"version": 1,
	"canisters": {
		"individual_user_template": {
			"type": "rust",
			"package": "individual_user_template",
			"candid": "./packages/ic/canister/individual_user_template/api/can.did",
			"declarations": {
				"output": "./packages/web-client/declarations/individual_user_template"
			}
		},
		"internet_identity": {
			"type": "custom",
			"candid": "internet_identity.did",
			"wasm": "internet_identity.wasm",
			"build": "./scripts/canisters/local_deploy/internet_identity.sh"
		},
		"post_cache": {
			"type": "rust",
			"package": "post_cache",
			"candid": "./packages/ic/canister/post_cache/api/can.did",
			"declarations": {
				"output": "./packages/web-client/declarations/post_cache"
			}
		},
		"project_member_index": {
			"type": "rust",
			"package": "project_member_index",
			"candid": "./packages/ic/canister/project_member_index/api/can.did",
			"declarations": {
				"output": "./packages/web-client/declarations/project_member_index"
			}
		},
		"user_index": {
			"type": "rust",
			"package": "user_index",
			"candid": "./packages/ic/canister/user_index/api/can.did",
			"declarations": {
				"output": "./packages/web-client/declarations/user_index"
			}
		},
		"webclient": {
			"frontend": {
				"entrypoint": "./packages/web-client/build/index.html"
			},
			"source": ["./packages/web-client/build"],
			"type": "assets"
		}
	}
}
