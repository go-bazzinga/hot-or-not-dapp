{
	"version": 1,
	"dfx": "0.11.2",
	"canisters": {
		"individual_user_template": {
			"type": "rust",
			"package": "individual_user_template",
			"candid": "./packages/canisters/individual_user_template/can.did",
			"declarations": {
				"output": "./packages/web-client/declarations/individual_user_template"
			}
		},
		"internet_identity": {
			"type": "custom",
			"candid": "internet_identity.did",
			"wasm": "internet_identity.wasm",
			"build": "./scripts/canisters/local_internet_identity_install.sh"
		},
		"post_cache": {
			"type": "rust",
			"package": "post_cache",
			"candid": "./packages/canisters/post_cache/can.did",
			"declarations": {
				"output": "./packages/web-client/declarations/post_cache"
			}
		},
		"user_index": {
			"type": "rust",
			"package": "user_index",
			"candid": "./packages/canisters/user_index/can.did",
			"declarations": {
				"output": "./packages/web-client/declarations/user_index"
			}
		},
		"webclient": {
			"frontend": {
				"entrypoint": "./packages/web-client/build/index.html"
			},
			"source": ["./packages/web-client/build"],
			"type": "assets"
		}
	},
	"defaults": {
		"build": {
			"packtool": "",
			"args": ""
		}
	},
	"networks": {
		"local": {
			"bind": "127.0.0.1:8000",
			"type": "ephemeral"
		}
	}
}
