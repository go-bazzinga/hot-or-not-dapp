{
	"version": 1,
	"dfx": "0.11.1",
	"canisters": {
		"individual_user_template": {
			"type": "rust",
			"package": "individual_user_template",
			"candid": "packages/canisters/individual_user_template/individual_user_template.did",
			"declarations": {
				"output": "packages/web-client/declarations/individual_user_template"
			}
		},
		"internet_identity": {
			"type": "custom",
			"candid": "internet_identity.did",
			"wasm": "internet_identity.wasm",
			"build": "./scripts/local_internet_identity_install.sh"
		},
		"user_index": {
			"type": "rust",
			"package": "user_index",
			"candid": "packages/canisters/user_index/user_index.did",
			"declarations": {
				"output": "packages/web-client/declarations/user_index"
			}
		},
		"webclient": {
			"frontend": {
				"entrypoint": "packages/web-client/build/index.html"
			},
			"source": ["packages/web-client/build"],
			"type": "assets"
		}
	},
	"defaults": {
		"build": {
			"packtool": "",
			"args": ""
		}
	},
	"networks": {
		"local": {
			"bind": "127.0.0.1:8000",
			"type": "ephemeral"
		}
	}
}
