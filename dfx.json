{
	"version": 1,
	"dfx": "0.11.0",
	"canisters": {
		"users": {
			"type": "rust",
			"package": "users",
			"candid": "packages/canisters/users/users.did",
			"declarations": {
				"output": "packages/web-client/declarations/users"
			}
		},
		"webclient": {
			"web-client": {
				"entrypoint": "packages/web-client/build/index.html"
			},
			"source": ["packages/web-client/build"],
			"type": "assets"
		}
	},
	"defaults": {
		"build": {
			"packtool": "",
			"args": ""
		}
	},
	"networks": {
		"local": {
			"bind": "127.0.0.1:8000",
			"type": "ephemeral"
		}
	}
}
