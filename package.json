{
	"name": "hot-or-not",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "npm run web-client:build",
		"web-client:build": "npm run --prefix ./packages/web-client build",
		"web-client:preview": "npm run --prefix ./packages/web-client preview",
		"web-client:dev": "npm run --prefix ./packages/web-client dev",
		"postinstall": "find ./packages/* -maxdepth 2 -name package.json -execdir npm install \\;"
	},
	"devDependencies": {
		"@sveltejs/eslint-config": "github:sveltejs/eslint-config#v5.8.0",
		"@typescript-eslint/eslint-plugin": "5.33.1",
		"@typescript-eslint/parser": "5.34.0",
		"eslint": "8.22.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "2.7.1",
		"prettier-plugin-tailwindcss": "0.1.13",
		"svelte": "3.49.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
