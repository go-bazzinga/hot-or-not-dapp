{
	"name": "@hnn/web-client",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npx playwright test",
		"test:vi": "vitest --coverage",
		"build:static": "BUILD_MODE=static vite build",
		"preview:static": "BUILD_MODE=static vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --ignore .svelte-kit,build,tests --threshold warning --diagnostic-sources \"js,ts,svelte\"",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"tsc": "tsc -b",
		"sync": "svelte-kit sync",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"dependencies": {
		"@builder.io/partytown": "0.7.3",
		"@dfinity/agent": "0.15.1",
		"@dfinity/auth-client": "0.15.0",
		"@dfinity/candid": "0.15.1",
		"@dfinity/identity": "0.15.0",
		"@dfinity/identity-secp256k1": "0.15.1",
		"@dfinity/principal": "0.15.1",
		"@esbuild-plugins/node-globals-polyfill": "0.1.1",
		"@esbuild-plugins/node-modules-polyfill": "0.1.4",
		"@sentry/svelte": "7.28.0",
		"@sentry/tracing": "7.28.0",
		"buffer": "6.0.3",
		"clsx": "1.2.1",
		"hls.js": "1.2.9",
		"idb": "7.1.1",
		"svelte-local-storage-store": "0.3.2",
		"swiper": "8.4.5",
		"throttle-debounce": "5.0.0"
	},
	"devDependencies": {
		"@playwright/test": "1.29.1",
		"@sveltejs/adapter-cloudflare": "1.0.0",
		"@sveltejs/adapter-static": "1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@tailwindcss/forms": "0.5.3",
		"@types/gtag.js": "0.0.12",
		"@types/lodash-es": "4.17.6",
		"@types/throttle-debounce": "5.0.0",
		"@types/w3c-image-capture": "1.0.7",
		"@vitest/coverage-c8": "^0.26.3",
		"@vitest/ui": "0.26.2",
		"autoprefixer": "10.4.13",
		"bip39": "3.0.4",
		"c8": "7.12.0",
		"hdkey": "2.0.1",
		"isomorphic-fetch": "3.0.0",
		"jsdom": "20.0.3",
		"postcss": "8.4.20",
		"svelte": "3.55.0",
		"svelte-check": "3.0.1",
		"svelte-easy-crop": "1.0.8",
		"svelte-preprocess": "5.0.0",
		"tailwindcss": "3.2.4",
		"ts-node": "10.9.1",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"vite": "4.0.2",
		"vite-tsconfig-paths": "3.5.0",
		"vitest": "0.26.1"
	}
