{
  "name": "@hnn/web-client",
  "version": "0.9.0",
  "type": "module",
  "private": true,
  "scripts": {
    "prebuild": "npm run sync",
    "build": "vite build",
    "prebuild:static": "npm run sync",
    "build:static": "BUILD_MODE=static vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json --ignore .svelte-kit,build,tests --threshold warning --diagnostic-sources \"js,ts,svelte\"",
    "cypress": "npx cypress open",
    "dev": "vite dev --host",
    "test": "npx cypress run",
    "test:record": "npx cypress run --record",
    "test:vi": "npm run test:vi:components && npm run test:vi:e2e",
    "test:vi:components": "npx vitest --watch false --coverage --config vitest.config.components.ts",
    "test:vi:e2e": "npx vitest  --watch false --config vitest.config.e2e.ts",
    "preview": "vite preview",
    "preview:static": "BUILD_MODE=static vite preview",
    "start": "node build",
    "sync": "svelte-kit sync",
    "tsc": "tsc -b"
  },
  "dependencies": {
    "@dfinity/agent": "1.0.1",
    "@dfinity/auth-client": "1.0.1",
    "@dfinity/candid": "1.0.1",
    "@dfinity/identity": "1.0.1",
    "@dfinity/identity-secp256k1": "1.0.1",
    "@dfinity/principal": "1.0.1",
    "@hnn/declarations": "*",
    "@hnn/components": "*",
    "@sentry/sveltekit": "7.107.0",
    "clsx": "2.1.0",
    "hls.js": "1.5.7",
    "idb": "8.0.0",
    "svelte-local-storage-store": "0.6.4",
    "throttle-debounce": "5.0.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "0.5.2",
    "@sveltejs/adapter-node": "5.0.1",
    "@sveltejs/adapter-static": "3.0.1",
    "@sveltejs/kit": "2.5.4",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@tailwindcss/forms": "0.5.7",
    "@types/gtag.js": "0.0.19",
    "@types/throttle-debounce": "5.0.2",
    "@types/w3c-image-capture": "1.0.10",
    "@vitest/ui": "1.3.1",
    "autoprefixer": "10.4.18",
    "c8": "9.1.0",
    "cypress": "13.7.0",
    "cypress-mochawesome-reporter": "3.8.2",
    "date-fns": "3.4.0",
    "dotenv": "16.4.5",
    "isomorphic-fetch": "3.0.0",
    "jsdom": "24.0.0",
    "postcss": "8.4.35",
    "svelte": "4.2.12",
    "svelte-check": "3.6.7",
    "svelte-easy-crop": "2.0.3",
    "svelte-preprocess": "5.1.3",
    "tailwindcss": "3.4.1",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "typescript": "5.4.2",
    "vite": "5.1.6",
    "vite-plugin-node-polyfills": "0.21.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.3.1"
  }
}
