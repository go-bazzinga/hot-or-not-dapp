{
  "name": "@hnn/web-client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "cypress": "npx cypress open",
    "preview": "vite preview",
    "test": "npx cypress run",
    "test:record": "npx cypress run --record",
    "test:vi": "npm run test:vi:components && npm run test:vi:e2e",
    "test:vi:components": "npx vitest --watch false --coverage --config vitest.config.components.ts",
    "test:vi:e2e": "npx vitest  --watch false --config vitest.config.e2e.ts",
    "build:static": "BUILD_MODE=static vite build",
    "preview:static": "BUILD_MODE=static vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --ignore .svelte-kit,build,tests --threshold warning --diagnostic-sources \"js,ts,svelte\"",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "tsc": "tsc -b",
    "sync": "svelte-kit sync",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@dfinity/agent": "^0.15.6",
    "@dfinity/auth-client": "^0.15.6",
    "@dfinity/candid": "^0.15.6",
    "@dfinity/identity": "^0.15.6",
    "@dfinity/identity-secp256k1": "^0.15.6",
    "@dfinity/principal": "^0.15.6",
    "@neoconfetti/svelte": "^1.0.0",
    "@sentry/svelte": "^7.56.0",
    "@sentry/tracing": "^7.56.0",
    "buffer": "^6.0.3",
    "clsx": "^1.2.1",
    "hls.js": "^1.4.6",
    "idb": "^7.1.1",
    "svelte-local-storage-store": "^0.5.0",
    "swiper": "^8.4.7",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.3.0",
    "@sveltejs/adapter-cloudflare": "^2.3.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.20.4",
    "@tailwindcss/forms": "^0.5.3",
    "@types/gtag.js": "^0.0.12",
    "@types/throttle-debounce": "^5.0.0",
    "@types/w3c-image-capture": "^1.0.7",
    "@vitest/coverage-c8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "c8": "^7.14.0",
    "cypress": "^12.15.0",
    "cypress-mochawesome-reporter": "^3.5.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "svelte": "^3.59.2",
    "svelte-check": "^3.4.3",
    "svelte-easy-crop": "^2.0.1",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-node-polyfills": "^0.9.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2"
  }
}
