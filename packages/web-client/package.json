{
  "name": "@hnn/web-client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npx cypress run",
    "test:record": "npx cypress run --record",
    "test:vi": "npm run test:vi:components && npm run test:vi:e2e",
    "test:vi:components": "npx vitest --watch false --coverage --config vitest.config.components.ts",
    "test:vi:e2e": "npx vitest  --watch false --config vitest.config.e2e.ts",
    "build:static": "BUILD_MODE=static vite build",
    "preview:static": "BUILD_MODE=static vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --ignore .svelte-kit,build,tests --threshold warning --diagnostic-sources \"js,ts,svelte\"",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "tsc": "tsc -b",
    "sync": "svelte-kit sync",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@builder.io/partytown": "0.7.5",
    "@dfinity/agent": "0.15.3",
    "@dfinity/auth-client": "0.15.3",
    "@dfinity/candid": "0.15.3",
    "@dfinity/identity": "0.15.3",
    "@dfinity/identity-secp256k1": "0.15.3",
    "@dfinity/principal": "0.15.3",
    "@sentry/svelte": "7.34.0",
    "@sentry/tracing": "7.34.0",
    "buffer": "6.0.3",
    "clsx": "1.2.1",
    "hls.js": "1.3.1",
    "idb": "7.1.1",
    "svelte-local-storage-store": "0.4.0",
    "swiper": "8.4.7",
    "throttle-debounce": "5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "2.0.1",
    "@sveltejs/adapter-static": "1.0.5",
    "@sveltejs/kit": "1.3.6",
    "@tailwindcss/forms": "0.5.3",
    "@types/gtag.js": "0.0.12",
    "@types/lodash-es": "4.17.6",
    "@types/throttle-debounce": "5.0.0",
    "@types/w3c-image-capture": "1.0.7",
    "@vitest/coverage-c8": "0.28.3",
    "@vitest/ui": "0.28.3",
    "autoprefixer": "10.4.13",
    "c8": "7.12.0",
    "cypress": "12.4.1",
    "cypress-mochawesome-reporter": "^3.2.3",
    "isomorphic-fetch": "3.0.0",
    "jsdom": "21.1.0",
    "postcss": "8.4.21",
    "svelte": "3.55.1",
    "svelte-check": "3.0.3",
    "svelte-easy-crop": "2.0.0",
    "svelte-preprocess": "5.0.1",
    "tailwindcss": "3.2.4",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.28.3"
  }
}
