{
  "name": "@hnn/web-client",
  "version": "0.9.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node build",
    "dev": "vite dev --host",
    "prebuild": "npm run sync",
    "build": "vite build",
    "cypress": "npx cypress open",
    "preview": "vite preview",
    "test": "npx cypress run",
    "test:record": "npx cypress run --record",
    "test:vi": "npm run test:vi:components && npm run test:vi:e2e",
    "test:vi:components": "npx vitest --watch false --coverage --config vitest.config.components.ts",
    "test:vi:e2e": "npx vitest  --watch false --config vitest.config.e2e.ts",
    "build:static": "BUILD_MODE=static vite build",
    "preview:static": "BUILD_MODE=static vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --ignore .svelte-kit,build,tests --threshold warning --diagnostic-sources \"js,ts,svelte\"",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "tsc": "tsc -b",
    "sync": "svelte-kit sync",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@dfinity/agent": "^0.20.2",
    "@dfinity/auth-client": "^0.20.2",
    "@dfinity/candid": "^0.20.2",
    "@dfinity/identity": "^0.20.2",
    "@dfinity/identity-secp256k1": "^0.20.2",
    "@dfinity/principal": "^0.20.2",
    "@hnn/declarations": "*",
    "@neoconfetti/svelte": "^2.2.0",
    "@sentry/sveltekit": "^7.90.0",
    "buffer": "^6.0.3",
    "clsx": "^2.0.0",
    "hls.js": "^1.4.11",
    "idb": "^8.0.0",
    "svelte-local-storage-store": "^0.6.4",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.0",
    "@sveltejs/adapter-cloudflare": "^3.0.1",
    "@sveltejs/adapter-node": "2.0.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/gtag.js": "^0.0.18",
    "@types/throttle-debounce": "^5.0.0",
    "@types/w3c-image-capture": "^1.0.10",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "c8": "^8.0.1",
    "cypress": "^13.6.1",
    "cypress-mochawesome-reporter": "^3.7.0",
    "date-fns": "^3.0.3",
    "dotenv": "^16.3.1",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.31",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-easy-crop": "^2.0.1",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-node-polyfills": "^0.17.0",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.0"
  }
}
