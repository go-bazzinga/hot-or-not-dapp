p {
  @apply py-2 text-sm text-white/70;
}
h4 {
  @apply py-2 text-lg text-white;
}

li {
  @apply marker:text-primary;
  @apply text-sm text-white/70;
}

th {
  @apply p-1 text-left text-white;
  @apply border-[1px];
  @apply border-white/50;
  @apply bg-white/10;
}

td {
  @apply border-[1px];
  @apply p-1;
  @apply border-white/50;
}

tr:nth-child(even) {
  @apply bg-white/10;
}
